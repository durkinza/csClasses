;entry:
1110 0111 0000 1111;    LDI r16, 0xFF
1110 0000 0001 0000;    LDI r17, 0x00
1110 1111 0010 1111;    LDI r18, -1
1011 1001 0000 0100;    OUT 0x04, r16	; enable writing for output 0x05
1101 0000 0000 1010;    rcall seven		; - - . . .
1101 0000 0001 0010;    rcall eight		; - - - . .
1101 0000 0001 1010;    rcall three		; . . . - -
1101 0000 0010 0010;    rcall two		; . . - - -
1101 0000 0010 1010;    rcall delay		; wait seven units before starting again 
1101 0000 0010 1001;    rcall delay 
1101 0000 0010 1000;    rcall delay 
1101 0000 0010 0111;    rcall delay 
1101 0000 0010 0110;    rcall delay 
1101 0000 0010 0101;    rcall delay 
1100 1111 1111 0001;	RJMP entry		; start the loop over again

;seven:            ; - - . . .
1101 0000 0011 0100;    RCALL dash
1101 0000 0011 0011;    RCALL dash
1101 0000 0010 1101;    RCALL dot
1101 0000 0010 1100;    RCALL dot
1101 0000 0010 1011;    RCALL dot
1101 0000 0001 1110;    RCALL delay
1101 0000 0001 1101;    RCALL delay
1101 0000 0001 1100;    RCALL delay
1001 0101 0000 1000;    RET

;eight:            ; - - - . .
1101 0000 0010 1011;    RCALL dash
1101 0000 0010 1010;    RCALL dash
1101 0000 0010 1001;    RCALL dash
1101 0000 0010 0011;    RCALL dot
1101 0000 0010 0010;    RCALL dot
1101 0000 0001 0101;    RCALL delay
1101 0000 0001 0100;	RCALL delay
1101 0000 0001 0011;	RCALL delay
1001 0101 0000 1000;    ret

;three:            ; . . . - -
1101 0000 0001 1101;    RCALL dot
1101 0000 0001 1100;    RCALL dot
1101 0000 0001 1011;    RCALL dot
1101 0000 0001 1111;    RCALL dash
1101 0000 0001 1110;    RCALL dash
1101 0000 0000 1100;    RCALL delay
1101 0000 0000 1011;    RCALL delay
1101 0000 0000 1010;    RCALL delay
1001 0101 0000 1000;    ret

;two:	          ; . . - - -
1101 0000 0001 0100;    RCALL dot
1101 0000 0001 0011;    RCALL dot
1101 0000 0001 0111;    RCALL dash
1101 0000 0001 0110;    RCALL dash
1101 0000 0001 0101;    RCALL dash
1101 0000 0000 0011;    RCALL delay
1101 0000 0000 0010;    RCALL delay
1101 0000 0000 0001;    RCALL delay
1001 0101 0000 1000;    ret

;delay:
1110 0111 0011 1111;    LDI r19, 255
1110 0111 0100 1111;    LDI r20, 255
1110 0011 0101 0010;    LDI r21, 50
;    BR:
0000 1111 0011 0010;		ADD r19, r18
1111 0111 1111 0001; 		BRBC 1, BR
0000 1111 0100 0010;		ADD r20, r18
0000 0000 0000 0000;		NOP
1111 0111 1101 1001;		BRBC 1, BR
0000 1111 0101 0010;		ADD r21, r18
0000 0000 0000 0000;		NOP
1111 0111 1100 0001;		BRBC 1, BR
1001 0101 0000 1000;	RET

;dot:
1101 0000 0000 1010;    RCALL led_on    ; turn on led
1101 1111 1111 0010;    RCALL delay	    ; wait one count
1101 0000 0000 1010;    RCALL led_off   ; turn off led
1101 1111 1111 0000;    RCALL delay	    ; wait one count
1001 0101 0000 1000;    RET				; return

;dash:
1101 0000 0000 0101;    RCALL led_on    ; turn on led
1101 1111 1110 1101;    RCALL delay	    ; wait two counts
1101 1111 1110 1100;    RCALL delay
1101 0000 0000 0100;    RCALL led_off   ; turn off led
1101 1111 1110 1010;    RCALL delay	    ; wait one count
1001 0101 0000 1000;    RET				; return

;led_on:
1011 1001 0000 0101;    OUT 5, r16		; set output to FF (on)
1001 0101 0000 1000;    RET				; return 

;led_off:
1011 1001 0001 0101;    OUT 5, r17		; set output to 00 (off)
1001 0101 0000 1000;    RET				; return
